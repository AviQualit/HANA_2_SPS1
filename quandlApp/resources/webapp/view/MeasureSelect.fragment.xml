<core:FragmentDefinition
	xmlns="sap.m"
	xmlns:core="sap.ui.core">
	<SelectDialog
		noDataText="No Products Found"
		title="Select Product"
		search="handleSearch"
		confirm="handleCloseMeasureDialog"
		cancel="handleCloseMeasureDialog"
		items="{
			path: '/'
		}" >
		<StandardListItem 
			title="{text}"
			description="{text}"
			
			iconInset="false"
			type="Active" />
	</SelectDialog>
</core:FragmentDefinition>